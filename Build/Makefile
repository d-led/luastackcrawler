endif
ifeq ($(config),debug_x32)
  stackcrawlertest_config = debug_x32
endif
ifeq ($(config),debug_x64)
  stackcrawlertest_config = debug_x64
endif
ifeq ($(config),release_native)
  stackcrawlertest_config = release_native
endif
ifeq ($(config),release_x32)
  stackcrawlertest_config = release_x32
endif
ifeq ($(config),release_x64)
  stackcrawlertest_config = release_x64
endif

